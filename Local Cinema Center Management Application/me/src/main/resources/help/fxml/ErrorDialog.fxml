<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="help.ErrorDialogController" prefHeight="200.0" prefWidth="400.0">
    <children>
        <Label fx:id="errorMessageLabel" layoutX="50.0" layoutY="50.0" text="Error Message" textFill="RED">
            <font>
                <Font name="Arial Bold" size="16.0" />
            </font>
        </Label>
        <Button layoutX="150.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleCloseButtonAction" text="Close" />
    </children>
</AnchorPane>